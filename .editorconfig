# EditorConfig is awesome: http://EditorConfig.org

# Top-most EditorConfig file
root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
indent_style = space
indent_size = 4

[*.html]
indent_style = space
indent_size = 1

[{package.json,package-lock.json,.travis.yml}]
indent_style = space
indent_size = 2
